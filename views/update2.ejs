                    </li>

                </ul>
            </div>

        </nav>
    </section>

    <section>

        <h1 class="heading">CHECK/UPDATE STATUS</h1>


    <section id="details">
        <div class="card">
            <h5 class="card-header">E-Hospital Bed Details</h5>

            <form class="form-add" method="post" action="/update2">

            <div class="card-body details-card">
                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Patient Name :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_name%>">
                    </div>
                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Patient Age :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_age%>">
                    </div>

                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Patient Aadhar No. :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_aadhar%>">
                    </div>

                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Contact No. :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_contact%>">
                    </div>

                </div>

                <hr>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Room No. :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_roomno%>">
                    </div>

                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Bed-No. :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_bedno%>">
                    </div>

                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Bed-Type :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_type%>">
                    </div>

                </div>

                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Date of Allotment :</label>
                    
                    <div class="col-sm-4">
                        <input type="text" class="form-control" readonly="true" placeholder="<%=vw_pt_date%>">
                    </div>

                </div>


                <div class="row details-btn" >
                    <div class="col-lg-6 col-md-6">
                        <button type="submit" class="btn btn-primary" style="margin-top: 5%;">Discharge</button> 
                    </div>
                </div>
              
            </div>

        </form>

        </div>

    </section>

    
   
</body>

